        </section>
        <section class="main-section">
            <div class="database-editor-section">
                <h1>Manage Database</h1>
                <ul>
                    <li><a href="">Coconut Damages</a></li>
                    <li><a href="">Farmers</a></li>
                </ul>
                <!-- first database  -->
                <div class="database-wrappers">
                    <h4>Management </h4>
                <div class="search-inputs-database">
                    <h2>Pest and or Disease </h2>
                    <div class="inputs-wrapper">
                        <div class="inputs-wrapper-inside">
                              <!-- <h5>Name contains</h5> -->
                            <div class="input-group mb-3" >
                                <input type="text" placeholder="Name contains.." class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                            </div>
                        </div>
                        <div class="inputs-wrapper-inside">
                                <!-- <h5>Scientific Name</h5> -->
                             <input type="text" placeholder="Input scientific name.." class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                        </div>
                         <button type="button" class="btn btn-success">SEARCH</button>
                    </div>
                </div>
                <div class="add-button-database-wrapper">
                    <button type="button" class="btn btn-success"> <span> + </span>Add New Pest and or Disease</button>
                </div>
                <div class="table-datasets-wrapper">
                    <div class="table-datasets-wrapper">
                        <table class="table">
                            <thead>
                              <tr>
                                <th scope="col">Name</th>
                                <th scope="col">Image</th>
                                <th scope="col">Scientific Name</th>
                                <th scope="col">Cause</th>
                                <th scope="col">Remark</th>
                                <th scope="col">Management</th>
                                <th scope="col">Action</th>
                              </tr>
                            </thead>
                            <tbody class="table-group-divider">
                            <% pestLists.forEach(pests => { %>
                              <tr>
                                <th ><%= pests.pestName %></th>
                                <td><img src="./images/rhinoceros-beetle.webp" alt=""></td>
                                <td><%= pests.scientificName %></td>
                                <td><%= pests.cause %></td>
                                <td><%= pests.remark %></td>
                                <td><%= pests.management %></td>
                              </tr>
                            <% }) %>
                        </tbody>
                            </table>
                </div>
                </div>
                <!-- second database  -->
                <div class="database-wrappers">
                    <h4>Management </h4>
                    <div class="add-button-database-wrapper">
                        <button type="button" class="btn btn-success"> <span> + </span>List a Farmer</button>
                    </div>
                    <div class="table-datasets-wrapper">
                        <table class="table">
                            <thead>
                            <tr>
                                <th scope="col">Farmer Name</th>
                                <th scope="col">Address</th>
                                <th scope="col">Company Name</th>
                                <th scope="col">Email</th>
                                <th scope="col">Password</th>
                                <th scope="col">Mobile Number</th>
                                <th scope="col">Action</th>
                            </tr>
                            </thead>
                            <tbody class="table-group-divider">
                            <tr>
                                <th>John Paul Dela Cruz</th>
                                <td>Corner Rizal Avenue and Valencia Streets, Puerto Princesa</td>
                                <td>Blackburrow Ranch</td>
                                <td>jpdelacruz@pcadrc.gov.ph</td>
                                <td>000000</td>
                                <td>0908-813-1496</td>
                                <td><a href=""><i class="fa-solid fa-pencil"></i></a><a href=""><i class="fa-solid fa-trash-can"></i></a></td>
                            </tr>
                            <tr>
